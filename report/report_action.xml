<odoo>
    <record id="paperformat_label_10x10" model="report.paperformat">
        <field name="name">Etiqueta 10x10 cm</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">100</field>
        <field name="page_width">100</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">0</field>
        <field name="margin_bottom">0</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">0</field>
        <field name="dpi">90</field>
    </record>

    <record id="action_report_label_10x10" model="ir.actions.report">
        <field name="name">Etiqueta Impresora 10x10</field>
        <field name="model">stock.picking</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">exe_reporte_impresora.report_label_10x10</field>
        <field name="report_file">exe_reporte_impresora.report_label_10x10</field>
        <field name="binding_model_id" ref="stock.model_stock_picking"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="paperformat_label_10x10"/>
    </record>
</odoo>